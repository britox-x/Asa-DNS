;
; Arquivo de dados do BIND para a zona asa.br
;
$TTL    604800
@       IN  SOA ns.asa.br. root.asa.br. (
                  3         ; Serial
             604800        ; Refresh
              86400        ; Retry
            2419200        ; Expire
             604800 )      ; Negative Cache TTL
;
@       IN  NS  	ns.asa.br.
ns      IN  A   	10.24.22.127
@       IN  CNAME   ns 
www     IN  CNAME  	ns
portal  IN  CNAME  	ns
